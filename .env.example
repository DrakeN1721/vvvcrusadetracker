# Development Mode - Set to true to bypass auth and use mock data
VITE_DEV_MODE=false

# Supabase Configuration (required when VITE_DEV_MODE=false)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# API Configuration (for legacy Cloudflare Workers - optional)
VITE_API_URL=http://localhost:8787/api

# Discord OAuth (handled by Supabase when VITE_DEV_MODE=false)
# Configure in Supabase Dashboard > Authentication > Providers
VITE_DISCORD_CLIENT_ID=your-discord-client-id
VITE_DISCORD_REDIRECT_URI=http://localhost:3000/api/auth/discord/callback